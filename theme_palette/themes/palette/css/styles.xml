<?xml version="1.0"?>
<styles version="1.0">
    <set name="Full Page">
        <style name="Page Background Color" variable="page-background" type="color" />
        <style name="Text" variable="body" type="type" />
        <style name="Text Sub Color" variable="body-text-sub" type="color" />
        <style name="Text White Color" variable="body-text-white" type="color" />
        <style name="Text Shadow Color" variable="body-text-shadow" type="color" />
        <style name="Link Color" variable="body-link" type="color" />
        <style name="Link Hover Color" variable="body-link-hover" type="color" />
        <style name="Main Background Color" variable="main-background" type="color" />
        <style name="Main Theme Color" variable="main-theme" type="color" />
        <style name="Main Base Color" variable="main-base" type="color" />
        <style name="Main Border Color" variable="main-border" type="color" />
        <style name="Page Titles" variable="page-title" type="type" />
    </set>
	<set name="Header">
		<style name="Header Background Color" variable="header-background" type="color" />
		<style name="Header Background Transparent" variable="header-background-transparent" type="color" />
		<style name="Header Offset" variable="header-offset" type="size" />
		<!-- <style name="Header Image Offset" variable="header-image-offset" type="size" /> -->
		<style name="Header Image Pattern" variable="header-background" type="image" />
		<style name="Site Title" variable="header-site-title" type="type" />
		<style name="Navigation" variable="header-navigation" type="type" />
        <style name="Navigation Hover" variable="header-navigation-hover" type="color" />
        <style name="Navigation Menu Background" variable="header-navigation-menu-background" type="color" />
        <style name="Navigation Menu Border" variable="header-navigation-menu-border" type="color" />
        <style name="Navigation Menu Text" variable="header-navigation-menu-text" type="color" />
        <style name="Navigation Menu Text Hover" variable="header-navigation-menu-text-hover" type="color" />
        <style name="Navigation Menu Margin Top" variable="header-navigation-menu-margin-top" type="size" />
	</set>
	<set name="Search">
        <style name="Search Field Background Color" variable="search-input-background" type="color" />
        <style name="Search Field Border Color" variable="search-input-border" type="color" />
        <style name="Search Button Color" variable="search-button" type="color" />
        <style name="Search Button Icon Color" variable="search-button-icon" type="color" />
	</set>
	<set name="Button">
        <style name="Button Text Color" variable="button-text" type="color" />
        <style name="Button Default Color" variable="button-default" type="color" />
        <style name="Button Primary Color" variable="button-primary" type="color" />
        <style name="Button Success Color" variable="button-success" type="color" />
        <style name="Button Large Text Color" variable="button-large-text" type="color" />
        <style name="Button Large Background Color" variable="button-large-background" type="color" />
	</set>
    <set name="Footer">
        <style name="Footer Text Color" variable="footer-text" type="color" />
        <style name="Footer Name Color" variable="footer-name" type="color" />
        <style name="Social Icons" variable="footer-social-icon" type="color" />
        <style name="Social Icons Hover" variable="footer-social-icon-hover" type="color" />
        <style name="Social Effect Icons" variable="footer-effect-social-icon" type="color" />

        <style name="Link Color" variable="footer-link" type="color" />
        <style name="Link Hover Color" variable="footer-link-hover" type="color" />
        <style name="Mobile Button Link Color" variable="footer-mobile-button-link" type="color" />
        <style name="Mobile Button Background Color" variable="footer-mobile-button-background" type="color" />
        <style name="Mobile Button Background Hover Color" variable="footer-mobile-button-background-hover" type="color" />
        <style name="Pagetop Button Color" variable="pagetop-button" type="color" />
        <style name="Pagetop Button Icon Color" variable="pagetop-button-icon" type="color" />
    </set>
    <set name="Headings">
        <style name="Heading 1" variable="h1" type="type" />
        <style name="Heading 2" variable="h2" type="type" />
        <style name="Heading 3" variable="h3" type="type" />
        <style name="Heading 4" variable="h4" type="type" />
        <style name="Heading 5" variable="h5" type="type" />
        <style name="Heading 6" variable="h6" type="type" />
    </set>
	<set name="Global Navigation">
		<style name="Global Navi Background Color" variable="global-navigation-background" type="color" />
		<style name="Global Navi Border Color" variable="global-navigation-border" type="color" />
		<style name="Global Navi Background Link Color" variable="global-navigation-background-link" type="color" />
		<style name="Global Navi Text Link Color" variable="global-navigation-text-link" type="color" />
		<style name="Global Navi Child Background Color" variable="global-navigation-child-background" type="color" />
		<style name="Global Navi Child Link" variable="global-navigation-child-link" type="color" />
		<style name="Global Navi Child Link Hover" variable="global-navigation-child-link-hover" type="color" />
		<style name="Global Navi Child Link Selected" variable="global-navigation-child-link-selected" type="color" />
		<style name="Global Navi Toggle Background Color" variable="global-navigation-toggle-background" type="color" />
		<style name="Global Navi Toggle Icon Color" variable="global-navigation-toggle-icon" type="color" />
		<style name="Global Navi Circle Left Color" variable="global-navigation-circle-left" type="color" />
		<style name="Global Navi Circle Center Color" variable="global-navigation-circle-center" type="color" />
		<style name="Global Navi Circle Right Color" variable="global-navigation-circle-right" type="color" />
	</set>
    <set name="Blockquote">
        <style name="Text" variable="blockquote" type="type" />
        <style name="Left Padding" variable="blockquote-left-padding" type="size" />
        <style name="Border Color" variable="blockquote-border" type="color" />
    </set>
    <set name="Image Slider">
        <style name="Slider Title" variable="image-slider-title" type="type" />
        <style name="Slider Paragraph" variable="image-slider-paragraph" type="type" />
        <style name="Slider Carousel Title" variable="image-slider-carousel-title" type="type" />
        <style name="Slider Carousel Paragraph" variable="image-slider-carousel-paragraph" type="type" />
    </set>
    <set name="Miscellaneous">
        <style name="Accent Background" variable="accent-background" type="color" />
        <style name="Accent White Background" variable="accent-white-background" type="color" />
        <style name="Title Thin" variable="title-thin" type="type" />
        <style name="Title Caps" variable="title-caps" type="type" />
        <style name="Title Caps Bold" variable="title-caps-bold" type="type" />
        <style name="Title Serif" variable="title-serif" type="type" />
        <style name="Map Title" variable="map-title" type="type" />
        <style name="Image Title" variable="image-title" type="type" />
        <style name="Social Icon" variable="social-icon" type="color" />
        <style name="Social Icon Background" variable="social-icon-background" type="color" />
        <style name="Carousel Photo Overlay" variable="carousel-photo-overlay" type="color" />
    </set>
</styles>

